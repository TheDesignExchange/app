    <owl:Class rdf:about="&DesignExchange_Methods;Atomize">
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Externalizing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DesignExchange_Methods;processesDataBy"/>
