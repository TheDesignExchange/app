    <owl:Class rdf:about="&DesignExchange_Methods;Person"/>
    


    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/DesignExchange_Methods#Personal_Inventory -->

    <owl:Class rdf:about="&DesignExchange_Methods;Personal_Inventory">
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Sampling-based"/>
