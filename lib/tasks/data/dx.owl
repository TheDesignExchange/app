    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/DesignExchange_Methods#Wireframes -->

    <owl:Class rdf:about="&DesignExchange_Methods;Wireframes">
        <rdfs:label>Wireframe</rdfs:label>
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;User_Interface_Prototypes"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&DesignExchange_Methods;hasFidelity"/>
