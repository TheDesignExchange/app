    <owl:Class rdf:about="&DesignExchange_Methods;Digital_Prototype">
        <rdfs:subClassOf rdf:resource="&DesignExchange_Methods;Prototype_Types"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/howard/ontologies/2014/0/DesignExchange_Methods#Digital_Prototype_Building -->

    <owl:Class rdf:about="&DesignExchange_Methods;Digital_Prototype_Building">
        <owl:equivalentClass>
