:css
  body {
    font-family: "Gotham", sans-serif;
  }
  h1, h2, h3 {
    font-weight: bold;
  }

  .about-header {
    text-align: center;
    color: #333333;
  }

  #logo-container {
    background-color: #59595C;
    padding: 20px;
    width: 100%;
    height: 250px;
  }

  #logo-img {
    width: 100%;
    height: 100%;
    object-fit: contain;
  }

  /*************/
  /* OUR STORY */

  .our-story-container {
    position: relative;
    display: flex;
    justify-content: center;
  }

  .our-story-content {
    display: flex;
    flex-direction: column;
    padding: 18px;
  }

  .our-story-text-right {
    margin-left: 8px;
  }

  .our-story-img-container {
    position: relative;
    width: 400px;
  }


  #about_1 {
    top: 30px;
    margin-bottom: 20px;
  }

  #about_2 {
    top: -24px;
  }


  .our-story-img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    box-shadow: 4px 4px 12px rgba(0,0,0,0.25);
    border-radius: 4px;
  }

  .img-back {
    position: absolute;
    border-radius: 4px;
  }

  #img-back-1 {
    left: 80px;
    top: -30px;
  }

  #img-back-2 {
    right: -50px;
    bottom: -20px;
  }

  #img-back-3 {
    left: -20px;
    bottom: -80px;
  }

  #img-back-4 {
    right: -20px;
    top: -30px;
  }

  #img-back-5 {
    width: 250px;
    height: 200px;
    right: -15px;
    bottom: -20px;
  }

  #img-back-6 {
    width: 200px;
    height: 150px;
    right: -30px;
    bottom: 10px;
  }

  .back-large {
    width: 350px;
    height: 250px;
    z-index: -1;
  }

  .back-small {
    width: 200px;
    height: 200px;
    z-index: -2;
  }

  .back-green {
    background-color: #6BC06D;
  }

  .back-grey {
    background-color: #F2F2F2;
  }

  /***********/
  /* MEMBERS */

  .members-container {
    background-color: #F2F2F2;
    padding: 18px;
    width: 100%;
    box-shadow: 2px 4px 18px rgba(0,0,0,0.12);
    border-radius: 4px;
  }

  .members-row {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;

  }

  .member {
    display:flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
    width: 180px;
    margin: 18px;
  }

  .member-img {
    background-color: #C4C4C4;
    width: 150px;
    height: 150px;
    border-radius: 50%;
    box-shadow: 3px 4px 12px rgba(0,0,0,0.12);
    margin-bottom: 12px;
  }

  .member-name {
    font-weight: bold;
  }


  /**********/
  /* VIDEOS */

  #video-container {
    display: flex;
    flex-direction: column;
    align-items: center;
  }

  .video-wrapper {
    color: #ffffff;
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 18px 0;
    background-color: #6BC06D;
    padding: 12px;
    border-radius: 4px;
    box-shadow: 4px 4px 12px rgba(0,0,0,0.25);


  }

  .video-frame {
    margin-bottom: 12px;
    border-radius: 4px;
  }




